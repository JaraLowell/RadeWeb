{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build RadegastWeb",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"RadeWeb.sln"
			],
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		},
		{
			"label": "Run RadegastWeb",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--project",
				"RadegastWeb.csproj"
			],
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		}
	]
}